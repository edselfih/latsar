<% layout('layout/template.ejs') %>
<h1 class="text-center mb-3" style="color: white">Detail Spby</h1>
<h4 class="text-center mb-3" style="color: white">
  Jenis: <%= inputedGups.jenisSpby %>
</h4>
<div class="d-flex justify-content-center">
    <div class="card text-left" style="width: 36rem;">
        <div class="card-body">
          <table class="table table-striped table-bordered">
            <tbody>
              <tr>
                <th>No Spby</th>
                <th><%= inputedGups.nomorSpby %></th>
              </tr>
              <tr>
                <th>No Nota Dinas</th>
                <th><%= inputedGups.nomorNd %></th>
              </tr>
              <tr>
                <th>No Surat Tugas</th>
                <th><%= inputedGups.nomorSt %></th>
              </tr>
              <tr>
                <th>Nominal Spby</th>
                <th
                <% if ( inputedGups.jumlahSpby !== jumlahKwitansi) { %> 
                    style="background-color: #dc3545"
                <% } %> 
                ><%= inputedGups.jumlahSpby %></th>
              </tr>
              <tr>
                <th>Nominal Kuitansi</th>
                <th
                <% if ( inputedGups.jumlahSpby !== jumlahKwitansi) { %> 
                    style="background-color: #dc3545"
                <% } %> 
                ><%= jumlahKwitansi %></th>
              </tr>
              <% for (let x in jenisSpby) { %>
              <tr>
                <th ><%= jenisSpby[x] %></th>
                <th><%= checked[x] === 'kosong' ? "-" : "v" %></th>
              </tr>
              <% } %>
                  <% if (checked && checked.duaSatu) { %><tr><th>Pajak 21</th> <th><%= checked.duaSatu  %><% } %></th></tr>
                  <% if (checked && checked.kjsDuaSatu) { %><tr><th>KJS 21</th> <th><%= checked.kjsDuaSatu  %><% } %></th></tr>
                  <% if (checked && checked.duaDua) { %><tr><th>Pajak 22</th> <th><%= checked.duaDua  %><% } %></th></tr>
                  <% if (checked && checked.kjsDuaDua) { %><tr><th>KJS 22</th> <th><%= checked.kjsDuaDua  %><% } %></th></tr>
                  <% if (checked && checked.duaTiga) { %><tr><th>Pajak 23</th> <th><%= checked.duaTiga  %><% } %></th></tr>
                  <% if (checked && checked.kjsDuaTiga) { %><tr><th>KJS 23</th> <th><%= checked.kjsDuaTiga  %><% } %></th></tr>
                  <% if (checked && checked.final) { %><tr><th>Pajak 4 ayat 2</th> <th><%= checked.final %><% } %></th></tr>
                  <% if (checked && checked.kjsPpn) { %><tr><th>KJS final</th> <th><%= checked.kjsPpn  %><% } %></th></tr>
                  <% if (checked && checked.ppn) { %><tr><th>Pajak PPN</th> <th><%= checked.ppn  %><% } %></th></tr>
                  <% if (checked && checked.kjsFinal) { %><tr><th>KJS PPN</th> <th><%= checked.kjsFinal  %><% } %></th></tr>
              <tr>
                <th>NPWP</th>
                <th><%= checked.npwp %></th>
              </tr>
              <tr>
                <th>Rekanan</th>
                <th><%= checked.rekanan %></th>
              </tr>
              
            </tbody>
          </table>
        </div>
      </div>
</div>

